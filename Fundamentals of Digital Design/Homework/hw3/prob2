v010aeca8_0 .net "inc", 0 0, v010aee08_0;  1 drivers
v010aed00_0 .var "value", 31 0;
E_010abdc0/0 .event edge, v010a33e8_0;
E_010abdc0/1 .event negedge, v02802488_0;
E_010abdc0 .event/or E_010abdc0/0, E_010abdc0/1;
S_010a4b60 .scope module, "dFlipFlop32Bits" "DFlipFlop32Bits" 2 4, 2 15 0, S_010a4a90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "clr"
    .port_info 2 /INPUT 32 "d"
    .port_info 3 /OUTPUT 32 "q"
v02802488_0 .net "clk", 0 0, v010aed58_0;  alias, 1 drivers
v010a3338_0 .net "clr", 0 0, v010aedb0_0;  alias, 1 drivers
v010a3390_0 .net "d", 31 0, v010a34f0_0;  1 drivers
v010a33e8_0 .var "q", 31 0;
E_010abf50/0 .event negedge, v02802488_0;
E_010abf50/1 .event posedge, v010a3338_0;
E_010abf50 .event/or E_010abf50/0, E_010abf50/1;
    .scope S_010a4b60;
T_0 ;
    %wait E_010abf50;
    %load/vec4 v010a3338_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v010a33e8_0, 0, 32;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v010a3390_0;
    %store/vec4 v010a33e8_0, 0, 32;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_010a4a90;
T_1 ;
    %wait E_010abdc0;
    %load/vec4 v010aeca8_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v010aec50_0;
    %addi 3, 0, 32;
    %store/vec4 v010a34f0_0, 0, 32;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v010aec50_0;
    %store/vec4 v010a34f0_0, 0, 32;
T_1.1 ;
    %load/vec4 v010aec50_0;
    %store/vec4 v010aed00_0, 0, 32;
    %jmp T_1;
    .thread T_1;
    .scope S_028023b8;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v010aedb0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v010aedb0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_028023b8;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v010aed58_0, 0, 1;
T_3.0 ;
    %delay 5, 0;
    %load/vec4 v010aed58_0;
    %inv;
    %store/vec4 v010aed58_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_028023b8;
T_4 ;
    %vpi_call 2 42 "$monitor", "%d inc=%b clk=%b clr=%b value=%d", $time, v010aee08_0, v010aed58_0, v010aedb0_0, v027fedf8_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v010aee08_0, 0, 1;
